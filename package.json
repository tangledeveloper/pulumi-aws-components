{
  "name": "pulumi-aws-components",
  "version": "0.0.1",
  "description": "Reusable Pulumi components ",
  "author": "Venkaiah Chowdary Koneru <koneru.chowdary@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "12.16.3",
    "npm": "6.14.4"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,md,json,yaml}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prepublish": "npm run lint && npm run build",
    "lint": "eslint . --ext .ts,.md,.json,.yaml",
    "pretest": "rimraf 'tests/**/*.{js,js.map,d.ts}'",
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@pulumi/aws": "2.9.0",
    "@pulumi/pulumi": "2.4.0",
    "@types/node": "12.12.47",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-markdown": "1.0.2",
    "husky": "4.2.5",
    "lerna": "3.22.1",
    "lint-staged": "10.2.10",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "typescript": "3.9.5"
  },
  "peerDependencies": {
    "@pulumi/aws": "2.9.0",
    "@pulumi/pulumi": "2.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koneru9999/pulumi-aws-components.git"
  }
}
